<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="POJO.Thongtingiaovu">
        <table name="thongtingiaovu" schema="db_quanlydkhp" catalog=""/>
        <attributes>
            <id name="idGiaoVu">
                <column name="idGiaoVu" length="12"/>
            </id>
            <basic name="hoVaTen">
                <column name="hoVaTen" length="45"/>
            </basic>
            <basic name="ngaySinh">
                <column name="ngaySinh" precision="-1"/>
            </basic>
            <basic name="gioiTinh">
                <column name="gioiTinh" length="3"/>
            </basic>
            <basic name="soDienThoai">
                <column name="soDienThoai" nullable="false" length="15"/>
            </basic>
            <basic name="email">
                <column name="email" nullable="false" length="45"/>
            </basic>
            <one-to-one name="taikhoanByIdGiaoVu" optional="true" target-entity="POJO.Taikhoan">
                <join-column name="idGiaoVu" referenced-column-name="idTaiKhoan"/>
            </one-to-one>
        </attributes>
    </entity>
</entity-mappings>
